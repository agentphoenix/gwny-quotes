{
	"require": {
		"laravel/framework": "4.2.*",
		"erusev/parsedown": "~1.0",
		"ikimea/browser": "~1.0",
		"laracasts/presenter": "0.1.*",
		"raveren/kint": "dev-master"
	},
	
	"require-dev": {
		"fzaninotto/faker": "~1.0"
	},
	
	"autoload": {
		"classmap": [
			"Golfwny/database/migrations",
			"Golfwny/database/seeds"
		],
		
		"psr-4": {
			"Golfwny\\Quote\\": "Golfwny\\Quote\\"
		}
	},
	
	"config": {
		"preferred-install": "dist",
		"process-timeout": 500
	},

	"minimum-stability": "dev",
	"prefer-stable": true
}