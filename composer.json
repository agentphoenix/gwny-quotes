{
	"require": {
		"laravel/framework": "~4.2.0",
		"league/commonmark": "*",
		"ikimea/browser": "~1.0",
		"laracasts/presenter": "~0.1.0"
	},
	
	"require-dev": {
		"raveren/kint": "dev-master",
		"fzaninotto/faker": "~1.0",
		"barryvdh/laravel-debugbar": "~1.0"
	},
	
	"autoload": {
		"classmap": [
			"Golfwny/database/migrations",
			"Golfwny/database/seeds"
